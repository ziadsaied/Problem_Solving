#include<iostream>
using namespace std;

int main() {
    const int size= 6;
    int numbers[size]={1,2,3,4,5,6};

    for(int i=0; i<size; ++i)
        cout<<numbers[i]<<" ";
    cout<<endl;

## to print backward using math:
    for(int i=0; i<size; ++i)
        cout<<numbers[size-i-1]<<" ";
## to print backward without math:
    for(int i = size - 1; i >= 0; --i)
        cout << numbers[i] << " ";
return 0;
}


